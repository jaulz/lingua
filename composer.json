{
    "name": "jaulz/lingua",
    "type": "library",
    "require": {
        "laravel/framework": "^9.51",
        "spatie/laravel-package-tools": "^1.14",
        "tpetry/laravel-postgresql-enhanced": "^0.26.0"
    },
    "license": "MPL-2.0",
    "autoload": {
        "psr-4": {
            "Jaulz\\Lingua\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jaulz\\Lingua\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Julian Hundeloh",
            "email": "github@julian.fyi"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4",
        "orchestra/testbench": "^7.22"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
